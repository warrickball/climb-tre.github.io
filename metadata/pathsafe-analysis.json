            "required": true,
            "actions": [
                "get",
                "list",
                "filter"
            ],
            "restrictions": [
                "Max length: 100"
            ]
        },
        "platform": {
            "description": "The platform used to sequence the data.",
            "type": "choice",
            "required": true,
            "actions": [
                "get",
                "list",
                "filter"
            ],
            "values": [
                "illumina"
            ]
        },
        "sample_accession": {
            "description": "Sample accession number if sequence is publically available in SRA.",
